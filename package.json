{
  "name": "web-boilerplate",
  "version": "2.0.0",
  "description": "nextjs boilerplate project",
  "main": "index.tsx",
  "scripts": {
    "dev": "dotenv -e .env.local next dev",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' dotenv -e .env.local next dev",
    "start": "dotenv -e .env.local next",
    "build:dev": "dotenv -e .env.development next build && next export",
    "build:prod": "dotenv -e .env.production next build && next export",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "deploy:dev": "serverless --stage=dev",
    "deploy:prod": "serverless --stage=prod",
    "lint:types": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "yarn lint -- --fix",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "unused": "next-unused",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@multiavatar/multiavatar": "^1.0.6",
    "@sentry/browser": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "antd": "^4.17.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^5.4.1",
    "i18next": "^21.5.4",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3",
    "react-use": "^17.3.1",
    "sass": "^1.44.0",
    "swr": "^1.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.11",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.4.0",
    "eslint-config-next": "^12.0.7",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "next-sitemap": "^1.6.203",
    "prettier": "^2.5.1",
    "serverless": "^2.68.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/",
      "/out/"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ]
  }
}
