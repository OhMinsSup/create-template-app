{
  "name": "web-boilerplate",
  "version": "2.0.0",
  "description": "nextjs boilerplate project",
  "main": "index.tsx",
  "scripts": {
    "dev": "dotenv -e .env.local next dev",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' dotenv -e .env.local next dev",
    "start": "dotenv -e .env.local next",
    "build:dev": "dotenv -e .env.development next build && next export",
    "build:prod": "dotenv -e .env.production next build && next export",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "deploy:dev": "serverless --stage=dev",
    "deploy:prod": "serverless --stage=prod",
    "lint:types": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "yarn lint -- --fix",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "unused": "next-unused",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@multiavatar/multiavatar": "^1.0.6",
    "@sentry/browser": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "antd": "^4.16.13",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "device-detector-js": "^2.2.10",
    "express": "^4.17.1",
    "framer-motion": "^4.1.17",
    "i18next": "^20.5.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^11.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-use": "^17.2.4",
    "sass": "^1.37.5",
    "swr": "^1.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@next/bundle-analyzer": "^11.0.1",
    "@serverless/core": "^1.1.2",
    "@serverless/template": "^1.1.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.4.12",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "husky": "^7.0.1",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.1",
    "next-sitemap": "^1.6.173",
    "next-unused": "^0.0.6",
    "prettier": "^2.3.2",
    "serverless": "^2.60.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/",
      "/out/"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ]
  },
  "next-unused": {
    "alias": {
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@libs/*": [
        "src/libs/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@contants/*": [
        "src/contants/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@locales/*": [
        "src/locales/*"
      ],
      "@provider/*": [
        "src/provider/*"
      ]
    },
    "debug": true,
    "include": [
      "src/contexts",
      "src/assets",
      "src/libs",
      "src/utils",
      "src/contants",
      "src/components",
      "src/hooks",
      "src/api",
      "src/pages",
      "src/locales",
      "src/provider"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
}
